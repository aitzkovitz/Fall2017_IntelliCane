<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false"><![CDATA[
    android:orientation="vertical"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:weightSum="3">



    ]]>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:padding="36dp"
        android:text="Administrator"
        android:textSize="15dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="101dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/buttonNewUser"
            android:layout_width="108dp"
            android:layout_height="93dp"
            android:layout_weight="0.09"
            android:onClick="newUser"
            app:srcCompat="@android:color/holo_green_light" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="108dp"
            android:layout_height="93dp"
            android:layout_weight="0.09"
            app:srcCompat="@android:color/holo_green_light" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="98dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="108dp"
            android:layout_height="93dp"
            android:layout_weight="0.09"
            app:srcCompat="@android:color/holo_green_light" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="108dp"
            android:layout_height="93dp"
            android:layout_weight="0.09"
            app:srcCompat="@android:color/holo_green_light" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="93dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="108dp"
            android:layout_height="93dp"
            android:layout_weight="0.09"
            app:srcCompat="@android:color/holo_green_light" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="108dp"
            android:layout_height="93dp"
            android:layout_weight="0.09"
            app:srcCompat="@android:color/holo_green_light" />
    </LinearLayout>

    <Button
        android:id="@+id/buttonNewUser1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="16dp"
        android:hint="Add New User"
        android:onClick="newUser" />

    <Button
        android:id="@+id/buttonDeleteUser"
        android:layout_width="145dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="16dp"
        android:hint="Associate Patient/Therapist"
        android:onClick="associateUser"
        android:layout_weight="0.01" />

    <Button
        android:id="@+id/buttonDeleteData"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="16dp"
        android:hint="Associate Patient/Session"
        android:onClick="associateSession" />

</LinearLayout>